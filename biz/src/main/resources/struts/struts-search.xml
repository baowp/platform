<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="com.abbcc.module.search" extends="json-default"
		namespace="/">
		<!--全局跳转-->
		<global-results>
			<result name="error">/user/error.jsp</result>
			<result name="login">/user/login.jsp</result>
			<result name="nopriv">/admin/nopriv.html</result>
		</global-results>

		

		<action name="*Search" class="com.abbcc.module.search.SearchAction"
			method="{1}">
			<result name="list">/search.jsp
			</result>
		</action>
		
	</package>



</struts>

