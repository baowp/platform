<struts>
	<package name="jmessage" namespace="/message" extends="abbcc-default">
		<action name="*" class="com.abbcc.module.message.JqueryMessageAction"
			method="{1}">
			<result name="mykx">/home/action/zx_mykx.jsp</result>
